{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
    "moduleResolution": "Node",
        "outDir": "./dist",
        "rootDir": "./server",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit":false
    },
    "include": [
        "server/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}